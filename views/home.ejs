<section class="search-sec">
  <div class="container">
      <form action="/searchResult" method="POST">
          <div class="row">
              <div class="col-lg-12">
                  <div class="row">
                    <div class="col-lg-3 col-md-3 col-sm-12 p-0 px-2">
                      <!-- <div class="input-group date"  data-target-input="nearest">
                        <input type="text" id="start_date" class="form-control search-slt datetimepicker-input input-sm" data-target="#start_date" placeholder="Start Date"/>
                        <div class="input-group-append" data-target="#start_date" data-toggle="datetimepicker">
                            <div class="input-group-text p-2"><i class="fa fa-calendar"></i></div>
                        </div>
                    </div> -->
                    <input name="search" class="form-control search-slt"  placeholder="Search text here"></input>
                  </div>
                    <div class="col-lg-3 col-md-3 col-sm-12 p-0 px-2">
                      <select type="text" name="floor" class="form-control search-slt" >
                        <option>Select Floor</option>
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                    </select>
                  </div>

                  <div class="col-lg-3 col-md-3 col-sm-12 p-0 px-2">
                    <select type="text" name="status" class="form-control search-slt" >
                      <option>Availabity</option>
                      <option>Available</option>
                      <option>Not Available</option>
                      <option>All</option>                      
                  </select>
                </div>
                  <div class="col-lg-3 col-md-3 col-sm-12 p-0 px-2">
                      <select type="text" name="capacity" class="form-control search-slt" >
                          <option>Capacity</option>
                          <option>10</option>
                          <option>15</option>
                          <option>20</option>
                          <option>50</option>
                      </select>
                  </div>

                  <div class="col">
                      <button type="submit" class="btn btn-light text-dark font-weight-bold wrn-btn">Search</button>
                  </div>
                  </div>
              </div>
          </div>
      </form>
  </div>
</section>
</div>
<div>
<!-- <div class="card-deck mb-3">result.length -->
  <% for(var i=0; i < 4 ; i++) { %>
    
    <div class="card" style="width: 24rem;">
        <!-- <img class="card-img-top" src="images/bg.jpg" alt="Card image cap"> -->
        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner active">
              <div class="carousel-item active">
                <img class="card-img-top d-block w-100" src="images/1.jpg" alt="First slide">
              </div>
              <div class="carousel-item">
                <img class="card-img-top d-block w-100" src="images/1.jpg" alt="Second slide">
              </div>
              <div class="carousel-item">
                <img class="card-img-top d-block w-100" src="images/1.jpg" alt="Third slide">
              </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        <div class="card-body">
          <h5 class="card-title"><%= result[i].room_id %></h5>
          <p class="card-text">A Well furnished office with all equipments needed for Succesfull connection with peers</p>
        </div>
        <ul class="list-group list-group-flush px-4">
          <li class="list-group-item px-4"><h4><strong><%= result[i].room_name %> room</h4></strong></li>
          <li class="list-group-item px-4">capcity:<%= result[i].capacity %></li>
          <li class="list-group-item px-4">Floor:<%= result[i].floor %></li>
          <li class="list-group-item px-4">Projector:<%= result[i].projector %></li>
          <li class="list-group-item px-4">Status:<%= result[i].status %></li>
        </ul>
        <div class="card-body">
            <!-- <i class="fas fa-shopping-cart"></i> -->
            <a href="#" id=<%= result[i].room_id %> onclick="addToCart('<%= result[i].room_id %>')" class="d-line btn btn-primary">Add to cart</a>
            <%if (data.isadmin=="admin") { %>
            <a href="http://localhost:3000/edit/<%= result[i].room_id%>" class="d-line btn btn-primary inline">Edit Room</a>
            <% } %>
        </div>
      </div>
        
        <% } %>
