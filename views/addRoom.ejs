<div>
<div class="container">
    <form class="form-group" action="/addRoom" method="POST">
    <!-- <img src="/images/<%=1%>.jpg" style="height: 200px; width: 200px"> -->
    <div class="form-group">
        <label><h4>Room ID</h4></label>
        <input name="room_id" class="form-control"  placeholder="Type Room id here"></input>
    </div>
    <div class="form-group">
        <label>Room Name</label>
        <input name="room_name" class="form-control" placeholder="Enter Metting Room Name"></input>
    </div>
    <div class="form-group">
        <label>Room Capacity</label>
        <input name="capacity" class="form-control" placeholder="Enter Capacity of Room"></input>
    </div>
    <div class="form-group">
        <label>Room Floor</label>
        <select type="text" name="floor" class="form-control search-slt" >
            <option>Select floor</option>
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4</option>
        </select>
    </div>
    <div class="form-group">
        <label>Projector</label>
        <input name="projector" class="form-control"  placeholder="Enter Number of Projecter"></input>
    </div>
    <div class="form-group">
        <label>Conference Ph no.</label>
        <input name="phone" class="form-control" placeholder="Enter Phone Number"></input>
    </div>
        <button type="submit" class="btn btn-info">Add Room</button>
        
    </form>
</div>
 
    
